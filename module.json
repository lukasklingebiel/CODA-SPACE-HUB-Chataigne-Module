      }
    },
    "Reverb Level":
    {
      "menu":"Reverb",  
      "callback":"reverbLevel",
      "parameters":
      {
        "Level":
        {
          "type":"Float",
          "min":-80,
          "max":10,
          "default":0
        }
      }
